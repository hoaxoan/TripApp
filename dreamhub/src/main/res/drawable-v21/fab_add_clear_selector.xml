<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/pressed_state"
        android:drawable="@drawable/ic_add_black_48dp_09"
        android:state_selected="true" />
    <item
        android:id="@+id/default_state"
        android:drawable="@drawable/ic_add_black_48dp_00" />

    <transition
        android:fromId="@id/default_state"
        android:toId="@+id/pressed_state">
        <animation-list>
            <item
                android:drawable="@drawable/ic_add_black_48dp_00"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_01"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_02"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_03"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_04"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_05"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_06"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_07"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_08"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_09"
                android:duration="15" />
        </animation-list>
    </transition>

    <transition
        android:fromId="@id/pressed_state"
        android:toId="@+id/default_state">
        <animation-list>
            <item
                android:drawable="@drawable/ic_add_black_48dp_09"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_08"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_07"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_06"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_05"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_04"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_03"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_02"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_01"
                android:duration="15" />
            <item
                android:drawable="@drawable/ic_add_black_48dp_00"
                android:duration="15" />
        </animation-list>
    </transition>

</animated-selector>